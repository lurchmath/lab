{
    "plugins": ["plugins/markdown"],
    "opts": {
        "template": "./node_modules/tidy-jsdoc",
        "prism-theme": "prism-default"
    },
    "templates": {
        "default": {
            "includeDate": false
        }
    },
    "metadata": {
        "title": "Lurch Deductive Engine"
    },
    "styles": {
        "nav-width": "335px"
    },	
    "source": {
        "exclude": [
            "./src/database/footer.js",
            "./src/database/generate.js",
            "./pre-docs.js",
            "./post-docs.js",
            "./examples/input/*.js"
        ]
    }
}